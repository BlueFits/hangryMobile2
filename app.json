{
  "expo": {
    "name": "hangryMobile",
    "slug": "hangryMobile",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.group.hangry",
      "infoPlist": {
        "NSCameraUsageDescription": "HangryMobile uses your camera to scan restaurant QR codes and view menu items"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.group.hangry"
    }
  }
}
